{
  "version": 2,
  "public": true,
  "headers": [
    {
      "source": "/(.*\\.xml)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/rss+xml; charset=utf-8"
        },
        {
          "key": "Cache-Control", 
          "value": "public, max-age=300"
        }
      ]
    }
  ]
} 